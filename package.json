{
	"name": "invenfinder-web",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"postinstall": "run-p install:* && husky install",
		"install:frontend": "cd ./frontend && npm install",
		"install:backend": "cd ./backend && npm install",
		"dev": "run-p dev:*",
		"dev:frontend": "cd ./frontend && npm run dev",
		"dev:backend": "cd ./backend && deno task start",
		"build": "run-p build:*",
		"build:frontend": "cd ./frontend && npm run build",
		"build:backend": "cd ./backend && deno task build",
		"preview": "cd ./frontend && npm run preview",
		"lint:frontend": "echo 'No linter setup'",
		"lint:backend": "cd ./backend && deno lint && deno fmt --check",
		"lint": "run-p lint:*",
		"fmt:frontend": "echo 'No formatter setup'",
		"fmt:backend": "cd ./backend && deno fmt",
		"fmt": "run-p fmt:*",
		"precommit": "npm run lint"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MStefan99/InvenFinder-web.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/MStefan99/InvenFinder-web/issues"
	},
	"homepage": "https://github.com/MStefan99/InvenFinder-web#readme",
	"devDependencies": {
		"husky": "^8.0.1",
		"npm-run-all": "^4.1.5"
	}
}
