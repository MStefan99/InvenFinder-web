{
	"printWidth": 100,
	"trailingComma": "none",
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"bracketSpacing": false,
	"bracketSameLine": true,
	"pugSingleQuote": false,
	"pugAttributeSeparator": "as-needed",
	"pugClassNotation": "literal",
	"pugIdNotation": "literal",
	"pugClassLocation": "before-attributes",
	"pugExplicitDiv": false
}
