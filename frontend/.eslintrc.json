{
	"env": {
		"browser": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:prettier/recommended",
		"plugin:vue/vue3-essential",
		"plugin:vue-pug/vue3-recommended",
		"@vue/typescript/recommended"
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"parser": "@typescript-eslint/parser",
		"sourceType": "module"
	},
	"rules": {
		"prettier/prettier": "warn"
	}
}
