{
	"env": {
		"browser": true,
		"node": true,
		"es2021": true
	},
	"extends": ["plugin:prettier/recommended", "plugin:vue/vue3-recommended"],
	"parser": "vue-eslint-parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"parser": "@typescript-eslint/parser",
		"sourceType": "module"
	},
	"plugins": ["vue", "@typescript-eslint", "prettier"],
	"rules": {
		"prettier/prettier": "warn"
	}
}
